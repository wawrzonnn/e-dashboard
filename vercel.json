{
    "routes": [
      {
        "src": "/_assets/(.*)",
        "headers": { "cache-control": "s-maxage=31536000,immutable" },
        "dest": "/_assets/$1"
      },
      {
        "src": "/src/(.*)",
        "headers": { "cache-control": "s-maxage=31536000,immutable" },
        "dest": "/src/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }